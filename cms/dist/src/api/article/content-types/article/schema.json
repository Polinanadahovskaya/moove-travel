{
    "kind": "collectionType",
    "collectionName": "articles",
    "info": {
        "singularName": "article",
        "pluralName": "articles",
        "displayName": "Article"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "Title": {
            "type": "string",
            "required": true
        },
        "Description": {
            "type": "text"
        },
        "Link": {
            "type": "string",
            "required": true
        },
        "user": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "plugin::users-permissions.user"
        },
        "articlePhotos": {
            "type": "media",
            "multiple": true,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "country": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::country.country"
        },
        "article_tags": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::article-tag.article-tag"
        },
        "PublishDate": {
            "type": "date"
        },
        "Content": {
            "type": "richtext"
        }
    }
}
